Cell["Formulation", "Section",
 CellChangeTimes->{{3.839522057042233*^9, 3.839522060290414*^9}},
 Background->RGBColor[
  0.87, 0.94, 1],ExpressionUUID->"368c296c-1256-4733-9868-70e61ea80816"],

Cell[TextData[{
 "\n",
 Cell[BoxData[
  RowBox[{
   RowBox[{"Vectors", " ", 
    FormBox[
     OverscriptBox[
      RowBox[{"p", " "}], "_"],
     TraditionalForm]}], " ", ",", " ", 
   FormBox[
    OverscriptBox["l1", "_"],
    TraditionalForm], ",", " ", 
   RowBox[{
    FormBox[
     OverscriptBox["l2", "_"],
     TraditionalForm], " ", "and", " ", 
    FormBox[
     OverscriptBox["l3", "_"],
     TraditionalForm], " ", "can", " ", "be", " ", "written", " ", "as"}], 
   ","}]],
  CellChangeTimes->{{3.8395323015697546`*^9, 3.839532305404333*^9}},
  ExpressionUUID->"500b6e04-7002-453d-94ab-81cb4cbe0211"],
 "\n",
 Cell[BoxData[{
  RowBox[{
   RowBox[{
    FormBox[
     RowBox[{"\[LongRightArrow]", "    ", 
      OverscriptBox[
       RowBox[{" ", "p", " "}], "_"], " "}],
     TraditionalForm], "=", "  ", 
    RowBox[{
     RowBox[{"[", 
      RowBox[{"x", ",", " ", "y", ",", " ", "z"}], "]"}], 
     FormBox[
      SuperscriptBox["", "T"],
      TraditionalForm]}]}], " "}], "\n", 
  RowBox[{
   RowBox[{
    RowBox[{"\[LongRightArrow]", "    ", 
     FormBox[
      OverscriptBox["l1", "_"],
      TraditionalForm]}], " ", "=", " ", 
    RowBox[{
     RowBox[{"l1", " ", "[", 
      RowBox[{"0", ",", " ", "0", ",", " ", "1"}], "]"}], 
     FormBox[
      SuperscriptBox["", "T"],
      TraditionalForm]}]}], " "}], "\n", 
  RowBox[{
   FormBox[
    RowBox[{"\[LongRightArrow]", "    ", 
     OverscriptBox["l2", "_"]}],
    TraditionalForm], " ", "=", " ", 
   RowBox[{
    RowBox[{"l2", " ", "[", 
     RowBox[{"c2c1", ",", " ", "c2s1", ",", " ", "s2"}], "]"}], 
    FormBox[
     SuperscriptBox["", "T"],
     TraditionalForm], " "}]}]}],
  CellChangeTimes->{{3.8395323015697546`*^9, 3.839532305404333*^9}},
  ExpressionUUID->"26a83557-c411-4b35-8c4e-a55cab8c5ebc"],
 "\n\[LongRightArrow] \t  ",
 Cell[BoxData[
  FormBox[
   OverscriptBox["l3", "_"], TraditionalForm]],
  CellChangeTimes->{{3.8395323015697546`*^9, 3.839532305404333*^9}},
  ExpressionUUID->"e43275e1-e0b1-4ed8-a365-08f8acb8833b"],
 "    =    l3  [c23c1, c23s1, s23]",
 Cell[BoxData[
  FormBox[
   SuperscriptBox["", "T"], TraditionalForm]],
  CellChangeTimes->{{3.8395323015697546`*^9, 3.839532305404333*^9}},
  ExpressionUUID->"6b2d1b1e-4f1e-4a97-a57d-69bb885fea4e"],
 "\nFrom loop closure, we get, \n",
 Cell[BoxData[
  RowBox[{
   FormBox[
    RowBox[{"\[LongRightArrow]", "            ", 
     OverscriptBox[
      RowBox[{"p", " "}], "_"], " "}],
    TraditionalForm], "=", " ", 
   RowBox[{
    FormBox[
     OverscriptBox["l1", "_"],
     TraditionalForm], " ", "+", " ", 
    FormBox[
     OverscriptBox["l2", "_"],
     TraditionalForm]}]}]],
  CellChangeTimes->{{3.8395323015697546`*^9, 3.839532305404333*^9}, {
   3.8395330132213993`*^9, 3.839533013377212*^9}},ExpressionUUID->
  "c39d150e-56cd-48e1-9d81-93342f530c6f"],
 " + ",
 Cell[BoxData[
  FormBox[
   OverscriptBox["l3", "_"], TraditionalForm]],
  CellChangeTimes->{{3.8395323015697546`*^9, 3.839532305404333*^9}},
  ExpressionUUID->"4686e57e-0d59-4b01-9c04-910498269aa7"],
 " \nSo, \n\[LongRightArrow]\t    x = c1 ( l2 c2 + l3 c23 ) \n\
\[LongRightArrow]        y = s1 ( l2 c2 + l3 c23 )\n\[LongRightArrow]        \
z = l1 + l2 s2 + l3 s23\n",
 Cell[BoxData[
  RowBox[{"\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"Thus", " ", "we", " ", "get", "  ", 
     OverscriptBox[
      RowBox[{"p", " "}], "_"]}], " ", "=", 
    RowBox[{
     RowBox[{"[", 
      RowBox[{"x", ",", " ", "y", ",", " ", "z"}], "]"}], 
     FormBox[
      SuperscriptBox["", "T"],
      TraditionalForm], " ", "and", " ", "complete", " ", "our", " ", 
     "forward", " ", "Kinematic", " ", "analysis"}]}]}]],
  CellChangeTimes->{{3.8395323015697546`*^9, 3.839532305404333*^9}, {
   3.8395330132213993`*^9, 3.839533013377212*^9}},ExpressionUUID->
  "9371597a-2cd0-45d7-b83a-576893d343a6"]
}], "Text",
 CellChangeTimes->{{3.839522074176622*^9, 3.8395220746128116`*^9}, {
  3.8395329114229193`*^9, 3.8395329594087677`*^9}, {3.839532990266718*^9, 
  3.8395332151316695`*^9}},ExpressionUUID->"795b3843-2081-4af7-9379-\
34270dc8c48c"],

Cell["\<\
\[Rule] Implementation of \[OpenCurlyDoubleQuote]fwdKin3RSpatial\
\[CloseCurlyDoubleQuote] function to solve forward kinematics equations for a \
spatial 3R serial manipulator.\
\>", "Text",
 CellChangeTimes->{{3.8389868484669495`*^9, 3.838986889695636*^9}, 
   3.8389872748041677`*^9, {3.838987347863451*^9, 3.8389873500040283`*^9}, {
   3.838987930607786*^9, 
   3.838987939574477*^9}},ExpressionUUID->"67a04759-7f7d-41e0-8f59-\
b3e17dc80392"],

Cell["\[Rule] Position of the base of the manipulator = {x0, y0, z0}.", "Text",
 CellChangeTimes->{{3.838987144410017*^9, 3.8389871511740513`*^9}, {
  3.838987183921384*^9, 3.838987239647672*^9}, {3.8389878693091607`*^9, 
  3.838987942293053*^9}},ExpressionUUID->"93d86b98-ec2b-46d5-9c25-\
202f78b7227c"],

Cell[BoxData[
 RowBox[{
  RowBox[{
   RowBox[{
    RowBox[{"fwdKin3RSpatial", "[", 
     RowBox[{
      RowBox[{"{", 
       RowBox[{"x0_", ",", "y0_", ",", "z0_"}], "}"}], ",", 
      RowBox[{"{", 
       RowBox[{"l1_", ",", "l2_", ",", "l3_"}], "}"}], ",", 
      RowBox[{"{", 
       RowBox[{"\[Theta]1_", ",", "\[Theta]2_", ",", "\[Theta]3_"}], "}"}], 
      " ", ",", 
      RowBox[{"{", 
       RowBox[{"x_", ",", "y_", ",", "z_"}], "}"}]}], "]"}], " ", ":=", 
    "\[IndentingNewLine]", 
    RowBox[{"Module", "[", 
     RowBox[{
      RowBox[{"{", "sol", "}"}], ",", "\[IndentingNewLine]", "\t\t\t", 
      RowBox[{
       RowBox[{"sol", " ", "=", " ", 
        RowBox[{"{", 
         RowBox[{
          RowBox[{"x", " ", "->", " ", 
           RowBox[{"x0", " ", "+", " ", 
            RowBox[{
             RowBox[{"Cos", "[", "\[Theta]1", "]"}], 
             RowBox[{"(", 
              RowBox[{
               RowBox[{"l2", " ", 
                RowBox[{"Cos", "[", "\[Theta]2", "]"}]}], " ", "+", " ", 
               RowBox[{"l3", " ", 
                RowBox[{"Cos", "[", 
                 RowBox[{"\[Theta]2", "+", "\[Theta]3"}], "]"}]}]}], 
              ")"}]}]}]}], ",", "\[IndentingNewLine]", "\t  ", 
          RowBox[{"y", " ", "->", " ", 
           RowBox[{"y0", " ", "+", " ", 
            RowBox[{
             RowBox[{"Sin", "[", "\[Theta]1", "]"}], 
             RowBox[{"(", 
              RowBox[{
               RowBox[{"l2", " ", 
                RowBox[{"Cos", "[", "\[Theta]2", "]"}]}], " ", "+", " ", 
               RowBox[{"l3", " ", 
                RowBox[{"Cos", "[", 
                 RowBox[{"\[Theta]2", "+", "\[Theta]3"}], "]"}]}]}], 
              ")"}]}]}]}], ",", "\[IndentingNewLine]", "\t  ", 
          RowBox[{"z", " ", "->", "  ", 
           RowBox[{"z0", " ", "+", " ", "l1", " ", "+", " ", 
            RowBox[{"l2", " ", 
             RowBox[{"Sin", "[", "\[Theta]2", "]"}]}], " ", "+", " ", 
            RowBox[{"l3", " ", 
             RowBox[{"Sin", "[", 
              RowBox[{"\[Theta]2", "+", "\[Theta]3"}], "]"}]}]}]}]}], "}"}]}],
        ";", "\[IndentingNewLine]", 
       RowBox[{"Return", "[", 
        RowBox[{"N", "[", "sol", "]"}], "]"}], ";"}]}], "\[IndentingNewLine]",
      "]"}]}], ";"}], "\[IndentingNewLine]"}]], "Input",
 CellChangeTimes->{{3.8389857129334793`*^9, 3.838985732869026*^9}, {
   3.8389858336549835`*^9, 3.8389858371383753`*^9}, {3.838986129529662*^9, 
   3.8389863516735506`*^9}, {3.8389864161178074`*^9, 3.8389864483680544`*^9}, 
   3.83898650508914*^9, {3.838986548287898*^9, 3.8389866196309705`*^9}, {
   3.838986686106739*^9, 3.8389867492802887`*^9}, {3.8389867872777677`*^9, 
   3.8389867964166574`*^9}, {3.8389869242344055`*^9, 
   3.8389869531000633`*^9}, {3.8389872428969545`*^9, 3.838987253987649*^9}, {
   3.838987284770747*^9, 3.838987285427181*^9}, {3.8389878370899935`*^9, 
   3.838987845994593*^9}, {3.8389879486509075`*^9, 3.8389879590042815`*^9}, {
   3.839224166839269*^9, 3.839224211906829*^9}, {3.839224251522614*^9, 
   3.8392242543964815`*^9}, {3.8392324214551764`*^9, 
   3.8392324388109107`*^9}, {3.839508483441626*^9, 3.839508493970414*^9}},
